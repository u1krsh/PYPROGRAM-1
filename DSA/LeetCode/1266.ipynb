   "source": [
    "point = [[1,1],[3,4],[-1,0]]\n",
    "result = 0\n",
    "x1,y1 = point.pop()\n",
    "while point:\n",
    "    x2,y2 = point.pop()\n",
    "    result += max(abs(y2-y1),abs(x2-x1))\n",
    "    x1,y1=x2,y2\n",
    "print(result)\n",
    "\n"
   ]
  }
